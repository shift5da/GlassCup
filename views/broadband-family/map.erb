<script type="text/javascript">

var map;
var marker;

$(document).ready(function () {
  map = new AMap.Map("my_map", {
    resizeEnable: true,
    mapStyle: 'amap://styles/grey',
    center: [
      121.499859, 31.23966
    ],
    zoom: 16
  });


  marker = new AMap.Marker({
    position: [
      121.442603, 31.205364
    ],
    title: '中汇花园',
    icon: '/images/icon/community-24.png',
    offset: new AMap.Pixel(-12, -12),
    map: map
  });

  AMap.event.addListener(marker, 'click', function () {
    window.location.href='/broadband-family/community-detail';
  });

  marker = new AMap.Marker({
    position: [
      121.441026, 31.206704
    ],
    title: '众联别墅',
    icon: '/images/icon/community-24.png',
    offset: new AMap.Pixel(-12, -12),
    map: map
  });

  AMap.event.addListener(marker, 'click', function () {
    window.location.href='/broadband-family/community-detail';
  });

  marker = new AMap.Marker({
    position: [
      121.44182, 31.206832
    ],
    title: '信联公寓',
    icon: '/images/icon/community-24.png',
    offset: new AMap.Pixel(-12, -12),
    map: map
  });

  AMap.event.addListener(marker, 'click', function () {
    window.location.href='/broadband-family/community-detail';
  });

  map.setCenter([121.442603, 31.205364]);

});



</script>
